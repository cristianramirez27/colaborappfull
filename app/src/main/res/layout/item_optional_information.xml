<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
            app:cardCornerRadius="8dp"
            android:layout_marginTop="@dimen/title_dashboard_top_calculator"
            android:layout_marginStart="@dimen/section_information_dashboard_sides_calculator"
            android:layout_marginEnd="@dimen/section_information_dashboard_sides_calculator"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" tools:layout_editor_absoluteX="1dp">
    <androidx.constraintlayout.widget.ConstraintLayout android:layout_width="match_parent"
                                                       android:layout_height="wrap_content"
                                                       android:background="@color/colorGrayCalculator">


        <TextView
                android:text="TextView"
                android:background="@android:color/white"
                android:layout_width="0dp"
                android:elevation="1dp"
                style="@style/TextMedium14CoppelCalculator"
                android:textAlignment="center"
                android:layout_height="wrap_content" android:id="@+id/textView66"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" android:paddingTop="20dp" android:paddingBottom="10dp"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView13"
                app:layout_constraintTop_toBottomOf="@+id/textView66" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginTop="@dimen/text_optional_information_sides_calculator"
                android:layout_marginBottom="@dimen/text_optional_information_sides_calculator"
                android:layout_marginStart="32dp"/>
        <TextView
                android:text="TextView"
                style="@style/TextBook12Coppel"
                android:textColor="@color/text_color_header_gte"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView67"
                app:layout_constraintStart_toEndOf="@+id/imageView13" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView66"
                android:layout_marginTop="@dimen/text_optional_information_sides_calculator"/>
        <TextView
                android:text="TextView"
                style="@style/TextBook16CoppelCalculator"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/textView68"
                app:layout_constraintStart_toEndOf="@+id/imageView13" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView67"
                android:layout_marginBottom="@dimen/text_optional_information_sides_calculator"
                android:layout_marginTop="8dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>